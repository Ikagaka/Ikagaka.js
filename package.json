    "ikagaka.named.js": "",
    "ikagaka.balloon.js": "",
    "ikagaka.sakurascriptplayer.js": "",
    "shiorijk": "",
    "ikagaka.ghost.js": ""
  },
  "devDependencies": {
    "uglify-js": ""
  },
